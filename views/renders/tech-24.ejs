<% layout("/layouts/boilerplate") %>


<H2 style="color:darkorange ; font-family: sans-serif; font-weight: 200; text-align: center; margin-top: 3rem;"  class="text-heading" >TECHNOVANZA 2024</H2>



      
<div class="gallery">

    


    <% for (let i = 1; i <= 43; i++) { %>
        <a href="#img-popup-<%= i %>" class="gallery-image">
            <img src="/images/2024/m<%= i %>.jpeg" alt="" class="image">
        </a>
        <!-- Hidden Popup Section for Each Image -->
        <div id="img-popup-<%= i %>" class="image-popup">
            <a href="#" class="close-popup">&times;</a>
            <img src="/images/2024/m<%= i %>.jpeg" class="large-popup-image" alt="Large Image">
        </div>
    <% } %>

    
</div>

    
    

















